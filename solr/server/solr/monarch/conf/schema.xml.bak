    <field name="object_class_secondary_closure_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="object_class_secondary_closure_label" dest="object_class_secondary_closure_label_searchable"></copyField>
    <!-- Automatically created to capture mapping information -->
    <!-- between object_class_secondary_(list|closure) and object_class_secondary_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="object_class_secondary_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: A unique identifier (CURIE) for the association. Optional. -->
    <!-- <field name="id" type="uuid" indexed="true" stored="true" /> -->
    <field name="id" type="string" multiValued="false" indexed="true" required="true" stored="true"/>
    <!-- generic_association: Category of association.object. Examples: phenotype, function, process, location, tissue, gene. In GO this is called aspect -->
    <field name="object_category" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Reflexive closure of association_has_object. A list of CURIEs. If an individual, first traverse rdf:tpye. The default closure is subclass, but other may optionally be added depending on what type of association this is and what the class is. E.g. for expression associations, the object is an anatomy class, and the closure will include part_of -->
    <field name="object_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Labels for object_class_closure. -->
    <field name="object_class_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- Automatically created to capture mapping information -->
    <!-- between object_class_(list|closure) and object_class_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="object_class_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: evidence object label -->
    <field name="evidence_object_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: List of CURIES of all entities that form part of the evidence graph. -->
    <field name="evidence_object" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Reflexive closure of gene. -->
    <field name="subject_gene_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Reflexive closure of gene. -->
    <field name="object_gene_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Labels for chromosome_closure. -->
    <field name="object_chromosome_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of object_chromosome_closure_label.-->
    <field name="object_chromosome_closure_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
