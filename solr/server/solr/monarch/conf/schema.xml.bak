    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="object_chromosome_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: Extension class for the annotation. -->
    <field name="object_extension_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: synonyms for the entity in the association.subject field. -->
    <field name="subject_synonym" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
