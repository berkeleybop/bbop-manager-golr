<?xml version="1.0" ?>
<schema name="golr" version="3.6">
  <types>
    <!--Unsplit string for when text needs to be dealt with atomically.-->
    <!--For example, faceted querying.-->
    <fieldType name="string" class="solr.StrField" sortMissingLast="true"></fieldType>
    <!--Any string with spaces that needs to be treated for searching purposes.-->
    <!--This will be automatically used in cases where "searchable: true" has been-->
    <!--specified in the YAML.-->
    <fieldType name="text_searchable" class="solr.TextField" positionIncrementGap="100" sortMissingLast="true">
      <analyzer type="index">
        <tokenizer class="solr.StandardTokenizerFactory"></tokenizer>
        <filter class="solr.LowerCaseFilterFactory"></filter>
        <filter class="solr.EdgeNGramFilterFactory" minGramSize="3" maxGramSize="15"/>
      </analyzer>
      <analyzer type="query">
        <tokenizer class="solr.StandardTokenizerFactory"></tokenizer>
      </analyzer>
    </fieldType>
    <fieldType name="integer" class="solr.TrieIntField" precisionStep="0" positionIncrementGap="0" sortMissingLast="true"></fieldType>
    <fieldType name="boolean" class="solr.BoolField" sortMissingLast="true"></fieldType>
    <fieldType name="uuid" class="solr.UUIDField" indexed="true" />
    <fieldType name="long" class="solr.TrieLongField"/>
    <fieldType name="strings" class="solr.StrField" sortMissingLast="true" multiValued="true"></fieldType>
    <fieldType name="booleans" class="solr.BoolField" sortMissingLast="true" multiValued="true"/>
    <fieldType name="tdates" class="solr.TrieDateField" positionIncrementGap="0" docValues="true" multiValued="true" precisionStep="6"/>
    <fieldType name="tlongs" class="solr.TrieLongField" positionIncrementGap="0" docValues="true" multiValued="true" precisionStep="8"/>
    <fieldType name="tdoubles" class="solr.TrieDoubleField" positionIncrementGap="0" docValues="true" multiValued="true" precisionStep="8"/>
  </types>
  <fields>
    <!-- A special static/fixed (by YAML conf file) field all documents have. -->
    <field name="document_category" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Label for association_subject. This will always be rdfs:label. Conventions may vary as to what goes in the label. For genes will be symbol, but we may choose to uniquify by prefixing with species -->
    <field name="subject_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of subject_label.-->
    <field name="subject_label_searchable" type="text_searchable" required="false" multiValued="false" indexed="true" stored="true"></field>
    <copyField source="subject_label" dest="subject_label_searchable"></copyField>
    <!-- generic_association: PANTHER families that are associated with this entity. -->
    <field name="subject_family_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of subject_family_label.-->
    <field name="subject_family_label_searchable" type="text_searchable" required="false" multiValued="false" indexed="true" stored="true"></field>
    <copyField source="subject_family_label" dest="subject_family_label_searchable"></copyField>
    <!-- generic_association: Rational for redundancy of annotation. -->
    <field name="is_redundant_for" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Extension class for the annotation (JSON). -->
    <field name="object_extension_json" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Labels for taxon_closure. -->
    <field name="object_taxon_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of object_taxon_closure_label.-->
    <field name="object_taxon_closure_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="object_taxon_closure_label" dest="object_taxon_closure_label_searchable"></copyField>
    <!-- Automatically created to capture mapping information -->
    <!-- between object_taxon_(list|closure) and object_taxon_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="object_taxon_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: labels for association_relation_closure -->
    <field name="relation_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- Automatically created to capture mapping information -->
    <!-- between relation_(list|closure) and relation_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="relation_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: Label of the chromosome. -->
    <field name="object_chromosome_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of object_chromosome_label.-->
    <field name="object_chromosome_label_searchable" type="text_searchable" required="false" multiValued="false" indexed="true" stored="true"></field>
    <copyField source="object_chromosome_label" dest="object_chromosome_label_searchable"></copyField>
    <!-- generic_association: Chromosome related to the subject. -->
    <field name="subject_chromosome" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Label of taxon. -->
    <field name="subject_taxon_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of subject_taxon_label.-->
    <field name="subject_taxon_label_searchable" type="text_searchable" required="false" multiValued="false" indexed="true" stored="true"></field>
    <copyField source="subject_taxon_label" dest="subject_taxon_label_searchable"></copyField>
    <!-- generic_association: Labels for gene_closure. -->
    <field name="subject_gene_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of subject_gene_closure_label.-->
    <field name="subject_gene_closure_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="subject_gene_closure_label" dest="subject_gene_closure_label_searchable"></copyField>
    <!-- Automatically created to capture mapping information -->
    <!-- between subject_gene_(list|closure) and subject_gene_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="subject_gene_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: Label for association_relation -->
    <field name="relation_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: citation. -->
    <field name="citation" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Ontologies that generated evidence axioms -->
    <field name="is_defined_by" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Taxonomic class of the object. This is typically a CURIE of the form NCBITaxon:nnnn. This field may be unfilled when used with certain categories -->
    <field name="object_taxon" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Evidence graph -->
    <field name="evidence_graph" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: Labels for chromosome_closure. -->
    <field name="subject_chromosome_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of subject_chromosome_closure_label.-->
    <field name="subject_chromosome_closure_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="subject_chromosome_closure_label" dest="subject_chromosome_closure_label_searchable"></copyField>
    <!-- Automatically created to capture mapping information -->
    <!-- between subject_chromosome_(list|closure) and subject_chromosome_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="subject_chromosome_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: Reflexive closure of association_has_object. A list of CURIEs. If an individual, first traverse rdf:tpye. The default closure is subclass, but other may optionally be added depending on what type of association this is and what the class is. E.g. for expression associations, the object is an anatomy class, and the closure will include part_of -->
    <field name="object_class_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Closure for evidence object -->
    <field name="evidence_object_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: A relationship type that connects the subject with object. TODO: check for correspondence in OBAN -->
    <field name="relation" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Label for association_object_category -->
    <field name="object_category_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Label for source -->
    <field name="source_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of source_label.-->
    <field name="source_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="source_label" dest="source_label_searchable"></copyField>
    <!-- generic_association: Extension class for the annotation. -->
    <field name="object_extension_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of object_extension_label.-->
    <field name="object_extension_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="object_extension_label" dest="object_extension_label_searchable"></copyField>
    <!-- generic_association: List of CURIES of all entities that form part of the evidence graph. -->
    <field name="evidence_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- Automatically created to capture mapping information -->
    <!-- between evidence_(list|closure) and evidence_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="evidence_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: CURIEs for source_closure -->
    <field name="source_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Label of taxon. -->
    <field name="object_taxon_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of object_taxon_label.-->
    <field name="object_taxon_label_searchable" type="text_searchable" required="false" multiValued="false" indexed="true" stored="true"></field>
    <copyField source="object_taxon_label" dest="object_taxon_label_searchable"></copyField>
    <!-- generic_association: Assoc object alternate form. E.g. for a canonical protein may be isoforms. Can we generalize this? May be deleted -->
    <field name="object_isoform" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Label for association_subject_category -->
    <field name="subject_category_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Labels for gene_closure. -->
    <field name="object_gene_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of object_gene_closure_label.-->
    <field name="object_gene_closure_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="object_gene_closure_label" dest="object_gene_closure_label_searchable"></copyField>
    <!-- Automatically created to capture mapping information -->
    <!-- between object_gene_(list|closure) and object_gene_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="object_gene_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: Chromosome related to the subject. -->
    <field name="object_chromosome" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Label of the gene. -->
    <field name="subject_gene_label" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of subject_gene_label.-->
    <field name="subject_gene_label_searchable" type="text_searchable" required="false" multiValued="false" indexed="true" stored="true"></field>
    <copyField source="subject_gene_label" dest="subject_gene_label_searchable"></copyField>
    <!-- generic_association: SubPropertyOf reflexive closure for association_relation -->
    <field name="relation_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Extension class for the annotation. -->
    <field name="object_extension_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of object_extension_closure_label.-->
    <field name="object_extension_closure_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="object_extension_closure_label" dest="object_extension_closure_label_searchable"></copyField>
    <!-- Automatically created to capture mapping information -->
    <!-- between object_extension_(list|closure) and object_extension_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="object_extension_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: List of CURIES of all entities that form part of the evidence closure. -->
    <field name="evidence_object_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- Automatically created to capture mapping information -->
    <!-- between evidence_object_(list|closure) and evidence_object_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="evidence_object_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
    <!-- generic_association: Closure for evidence_type. Always SubClassof -->
    <field name="evidence_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Reflexive closure of taxon. ALWAYS up SubClassOf. -->
    <field name="subject_taxon_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: TBD: ALIGN GO AND DC TERMINOLOGY. Source of association. The association may be derived from multiple source, but this should be the 'outer' source, if applicable -->
    <field name="assigned_by" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Extension class for the annotation. -->
    <field name="object_extension" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: The CURIE for oban:association_has_subject. May be disease, variant, gene, person, .... -->
    <field name="subject" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: A more descriptive label or full name for association_subject. For a gene this may be the full name (as opposed to symbol). -->
    <field name="subject_description" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of subject_description.-->
    <field name="subject_description_searchable" type="text_searchable" required="false" multiValued="false" indexed="true" stored="true"></field>
    <copyField source="subject_description" dest="subject_description_searchable"></copyField>
    <!-- generic_association: Taxonomic class of the subject. This is typically a CURIE of the form NCBITaxon:nnnn. -->
    <field name="subject_taxon" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Evidence type. In Monarch we may have a graph/chain. This is always the 'outer' evidence OR a list of all evidence types used? -->
    <field name="evidence" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Association qualifier. TBD: NEGATION should probably be handled differently somehow -->
    <field name="qualifier" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Optional. This is similar to object_class_closure, but allows for the scenario whereby a different (more inclusive, or exclusive) closure is used. The exact OPs used will depend on the association type. This would ideally be communicating from loader conf through to the client somehow -->
    <field name="object_class_secondary_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: The CURIE for oban:association_has_object. This is often, but not always an ontology class. E.g. for a gene-gene interaction it is an (arbitrary) member of the pair. -->
    <field name="object" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Date of assignment. The association may be derived from multiple source, but this should be the 'outer' source, if applicable. -->
    <field name="date" type="string" required="false" multiValued="false" indexed="true" stored="true"></field>
    <!-- generic_association: Reflexive closure of association_has_subject. A list of CURIEs. If an individual, first traverse rdf:tpye. The default closure is subclass, but other may optionally be added depending on what type of association this is and what the class is. E.g. for expression associations, the object is an anatomy class, and the closure will include part_of -->
    <field name="subject_closure" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!-- generic_association: Labels for subject_closure. -->
    <field name="subject_closure_label" type="string" required="false" multiValued="true" indexed="true" stored="true"></field>
    <!--An easily searchable (TextField tokenized) version of subject_closure_label.-->
    <field name="subject_closure_label_searchable" type="text_searchable" required="false" multiValued="true" indexed="true" stored="true"></field>
    <copyField source="subject_closure_label" dest="subject_closure_label_searchable"></copyField>
    <!-- Automatically created to capture mapping information -->
    <!-- between subject_(list|closure) and subject_closure_label.-->
    <!-- It is not indexed for searching (JSON blob), but may be useful to the client. -->
    <field name="subject_closure_map" type="string" required="false" multiValued="false" indexed="false" stored="true"></field>
